{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "github": {
    "silent": true
  },
  "redirects": [
    { "source": "/", "destination": "https://github.com/masan4444/github-action-test" },
    { "source": "/install", "destination": "https://raw.githubusercontent.com/masan4444/github-action-test/master/.github/workflows/release.yml" }
  ],
  "headers": [
    {
      "source": "/install",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
